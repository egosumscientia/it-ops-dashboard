generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  // La URL ya no se debe configurar aqu√≠
}

model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  password  String
  createdAt DateTime @default(now())
}

model Incident {
  id        Int      @id @default(autoincrement())
  title     String
  status    String
  priority  String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
